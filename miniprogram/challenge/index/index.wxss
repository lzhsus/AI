page {
  background: rgba(238, 238, 238, 0.1);
}
#index .page-index .header {
  width: 700rpx;
  height: 300rpx;
  position: relative;
  margin: 30rpx auto;
  background: #fff;
  border-radius: 30rpx;
  box-shadow: 0 0 20rpx rgba(221, 119, 119, 0.1);
}
#index .page-index .header .info {
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  font-size: 12px;
  color: #000;
}
#index .page-index .header .info text {
  color: red;
  font-size: 20px;
  font-weight: bold;
}
#index .page-index .header .ranking {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  font-size: 12px;
  color: #000;
}
#index .page-index .header .user-msg-box {
  width: 100%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  justify-content: center;
  flex-direction: column;
}
#index .page-index .header .user-msg-box .cuserAvatarUrl {
  display: block;
  width: 142rpx;
  height: 142rpx;
  background: #fff;
  border: 3px solid #ccc;
  box-sizing: border-box;
  border-radius: 50%;
  overflow: hidden;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
#index .page-index .header .user-msg-box .cuserAvatarUrl view {
  display: block;
  width: 126rpx;
  height: 126rpx;
  overflow: hidden;
  border-radius: 50%;
}
#index .page-index .header .user-msg-box .userNickName {
  font-size: 18px;
  color: #000;
  font-weight: bold;
  text-align: center;
  margin-top: 10rpx;
}
#index .page-index .main {
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  align-items: center;
}
#index .page-index .main .item {
  width: 700rpx;
  height: 150rpx;
  box-shadow: 0 0 12rpx rgba(214, 206, 206, 0.5);
  background: #4974c7;
  border-radius: 12rpx;
  padding: 14rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
#index .page-index .main .item.unlock-success {
  background: #9e7936;
}
#index .page-index .main .item.unlock-success .item-main .title-icon {
  background-position: center bottom;
}
#index .page-index .main .item .item-main {
  width: 100%;
  height: 100%;
  border: 1px dashed #fff;
  box-sizing: border-box;
  position: relative;
}
#index .page-index .main .item .item-main .lock-icon {
  position: absolute;
  top: 6px;
  right: 12rpx;
  width: 30rpx;
  height: 30rpx;
  background: url(https://6c7a-lzhsus-1g4h29bs69c66542-1301447037.tcb.qcloud.la/font_num/suo-icon.png?v1) center center no-repeat;
  background-size: 100% auto;
}
#index .page-index .main .item .item-main .grade-icon {
  position: absolute;
  top: 12rpx;
  right: 50rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex-wrap: nowrap;
}
#index .page-index .main .item .item-main .grade-icon span {
  display: block;
  width: 20rpx;
}
#index .page-index .main .item .item-main .grades-icon {
  position: absolute;
  bottom: 12rpx;
  right: 12rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
#index .page-index .main .item .item-main .grades-icon span {
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin-left: 2px;
  background: url('https://6c7a-lzhsus-1g4h29bs69c66542-1301447037.tcb.qcloud.la/font_num/wujiaox-icon.png') center center no-repeat;
  background-size: cover;
}
#index .page-index .main .item .item-main .title-icon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 300rpx;
  height: 93rpx;
  background: url(https://6c7a-lzhsus-1g4h29bs69c66542-1301447037.tcb.qcloud.la/font_num/title-icon.png?sign=3d3a9a3be5c16e02bf43e967aaccd549&t=1629178371) center top no-repeat;
  background-size: 100% auto;
}
#index .page-index .main .item .item-main .title-icon.active {
  background-position: center bottom;
}
