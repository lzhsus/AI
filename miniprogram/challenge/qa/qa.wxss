page {
  background: #f1eded;
}
#qaId {
  overflow: hidden;
}
#qaId .main-box {
  width: 650rpx;
  margin: 20rpx auto;
  padding: 30rpx;
  box-sizing: border-box;
  border-radius: 30rpx;
  overflow: hidden;
  position: relative;
  background: #fff;
  box-shadow: 0 0 20rpx rgba(241, 237, 237, 0.5);
}
#qaId .main-box .h1 {
  width: 100%;
  height: 70px;
  font-size: 20px;
  color: #0d86d6;
  font-weight: bold;
  text-align: left;
}
#qaId .main-box .sel-item {
  width: 100%;
  text-align: left;
  padding: 26rpx;
  background: #f5f5f5;
  box-sizing: border-box;
  font-size: 12px;
  color: #000;
  font-weight: bold;
  margin-bottom: 20rpx;
  border-radius: 4px;
  overflow: hidden;
  display: flex;
  justify-content: stretch;
}
#qaId .main-box .sel-item .key {
  display: block;
  width: 20px;
  min-width: 20px;
}
#qaId .main-box .sel-item.choice {
  color: red;
}
#qaId .main-box .sel-item.choice .key {
  color: #000;
}
#qaId .main-box .sel-item:last-child {
  margin-bottom: 0;
}
#qaId .main-box .isQaResult {
  position: absolute;
  bottom: 50rpx;
  right: 20rpx;
  font-size: 20px;
  color: #409eff;
  font-weight: bold;
  transform: rotate(-45deg);
}
#qaId .main-box .isQaResult.red {
  color: red;
}
#qaId .main-box .hint {
  padding: 40rpx 0 0;
  font-size: 10px;
  text-align: center;
  color: #999;
}
.pop {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
}
.pop .pop-contnet {
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 650rpx;
  padding: 30rpx;
  box-sizing: border-box;
  border-radius: 30rpx;
  background: #fff;
}
.pop .pop-contnet .title {
  font-size: 30px;
  font-weight: bold;
  color: #000;
  text-align: center;
  width: 100%;
}
.pop .pop-contnet .main {
  width: 500rpx;
  margin: 50rpx auto 0;
}
.pop .pop-contnet .main .item {
  display: flex;
  font-size: 12px;
  color: #303030;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.pop .pop-contnet .main .item .key {
  flex-shrink: 0;
}
.pop .pop-contnet .btn-box {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
}
.pop .pop-contnet .btn-box van-button,
.pop .pop-contnet .btn-box button {
  width: 200rpx;
  display: block;
  line-height: 30px;
}
