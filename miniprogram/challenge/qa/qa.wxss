page {
  background: #f1eded;
}
#qaId {
  overflow: hidden;
}
#qaId .main-box {
  width: 650rpx;
  margin: 20rpx auto;
  padding: 30rpx;
  box-sizing: border-box;
  border-radius: 30rpx;
  overflow: hidden;
  background: #fff;
  box-shadow: 0 0 20rpx rgba(241, 237, 237, 0.5);
}
#qaId .main-box .h1 {
  width: 100%;
  height: 70px;
  font-size: 20px;
  color: #0d86d6;
  font-weight: bold;
  text-align: left;
}
#qaId .main-box .sel-item {
  width: 100%;
  text-align: left;
  padding: 26rpx;
  background: #f5f5f5;
  box-sizing: border-box;
  font-size: 12px;
  color: #000;
  font-weight: bold;
  margin-bottom: 20rpx;
  border-radius: 4px;
  overflow: hidden;
  display: flex;
  justify-content: stretch;
}
#qaId .main-box .sel-item .key {
  display: block;
  width: 20px;
  min-width: 20px;
}
#qaId .main-box .sel-item.choice {
  color: red;
}
#qaId .main-box .sel-item.choice .key {
  color: #000;
}
#qaId .main-box .sel-item:last-child {
  margin-bottom: 0;
}
#qaId .main-box .hint {
  padding: 40rpx 0 0;
  font-size: 10px;
  text-align: center;
  color: #999;
}
