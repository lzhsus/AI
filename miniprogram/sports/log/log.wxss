#loglist .head-main {
  width: 100%;
  height: 200rpx;
}
#loglist .head-main .head-top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 200rpx;
  background: #ffffff;
}
#loglist .list {
  padding: 0 50rpx;
  box-sizing: border-box;
}
#loglist .list .li {
  border-bottom: 1px solid #e9e9e9;
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
#loglist .list .li:last-child {
  margin-bottom: 0;
}
#loglist .list .li .step {
  width: 160rpx;
  font-size: 18px;
  color: #000;
  font-weight: bold;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
#loglist .list .li .step text {
  font-size: 14px;
}
#loglist .list .li .time {
  font-size: 12px;
  color: #9b9b9b;
}
